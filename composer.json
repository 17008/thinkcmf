{
    "name": "thinkcmf/thinkcmf",
    "description": "ThinkCMF based on ThinkPHP 5.1 , it is a free and open source Content Management Framework(CMF)",
    "type": "project",
    "keywords": [
        "cmf",
        "thinkcmf",
        "framework",
        "thinkphp",
        "ORM"
    ],
    "homepage": "http://www.thinkcmf.com/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "thinkcmf",
            "email": "catman@thinkcmf.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-pdo": "*",
        "topthink/framework": "~5.1.0",
        "topthink/think-helper": "^1.0",
        "topthink/think-captcha": "^2.0",
        "phpmailer/phpmailer": "^5.2",
        "mindplay/annotations": "^1.3",
        "topthink/think-image": "^1.0",
        "qiniu/php-sdk": "^7.1",
        "dompdf/dompdf": "^0.8.0",
        "phpoffice/phpexcel": "^1.8",
        "ezyang/htmlpurifier": "^4.9",
        "topthink/think-queue": "^2.0",
        "electrolinux/phpquery": "^0.9.6",
        "phpoffice/phpspreadsheet": "^1.5",
        "thinkcmf/cmf": "5.1.*-dev",
        "thinkcmf/cmf-app": "5.1.*-dev",
        "thinkcmf/cmf-swoole": "5.1.*-dev",
        "thinkcmf/cmf-api": "5.1.*-dev"
    },
    "extra": {
        "think-path": "vendor/thinkphp"
    },
    "config": {
        "vendor-dir": "vendor"
    },
    "autoload": {
        "classmap": [
            "vendor/thinkcmf/cmf/src/App.php",
            "vendor/thinkcmf/cmf/src/Log.php",
            "vendor/thinkcmf/cmf/src/route/dispatch/Module.php"
        ]
    }
}
